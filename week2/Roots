#include<stdio.h>

#include<math.h>

int main(){

int a,b,c,p,s;

float d;

d=(b*b)-(4*a*c);

scanf("%d%d%d",&a,&b,&c);

p=(-b+sqrt(d))/2;

s=(-b-sqrt(d))/2;

if(d>0)

printf("the roots are real:%d,%d",p,s);

else if(d==0)

printf("the roots are equal:%d,%d",p,s);

else

printf("the roots are imaginary");
  }
